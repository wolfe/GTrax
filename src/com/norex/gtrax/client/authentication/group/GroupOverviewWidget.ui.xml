<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
        xmlns:gtrax='urn:import:com.norex.gtrax.client'>
        

	        	<gwt:VerticalPanel width="100%">
	        		<gwt:TextBox ui:field="name" />
	        		<gwt:Anchor ui:field="remove" text="remove" />
	        		<gwt:DisclosurePanel ui:field="members">
	        			<gwt:VerticalPanel>
	        				<gwt:HorizontalPanel>
	        					<gtrax:AuthSuggestBox ui:field="add" />
	        					<gwt:Button ui:field="addButton" text="add" />
	        				</gwt:HorizontalPanel>
	        				
	        			</gwt:VerticalPanel>
	        		</gwt:DisclosurePanel>
	        	</gwt:VerticalPanel>
        
</ui:UiBinder>