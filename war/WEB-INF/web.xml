<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>AuthServiceImpl</servlet-name>
    <servlet-class>com.norex.gtrax.server.AuthServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>AuthServiceImpl</servlet-name>
    <url-pattern>/gtrax/auth</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>ProjectServiceImpl</servlet-name>
    <servlet-class>com.norex.gtrax.server.ProjectServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ProjectServiceImpl</servlet-name>
    <url-pattern>/gtrax/project</url-pattern>
  </servlet-mapping>
  
  
    <servlet>
    <servlet-name>ContactServiceImpl</servlet-name>
    <servlet-class>com.norex.gtrax.server.ContactServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ContactServiceImpl</servlet-name>
    <url-pattern>/gtrax/contact</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>fileUploaderServlet</servlet-name>
    <servlet-class>com.norex.gtrax.server.FileUpload</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>fileUploaderServlet</servlet-name>
  <url-pattern>/gtrax/fileupload</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>blobRenderServlet</servlet-name>
    <servlet-class>com.norex.gtrax.server.BlobRender</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>blobRenderServlet</servlet-name>
  <url-pattern>/gtrax/blobrender</url-pattern>
</servlet-mapping>
  
  
</web-app>
